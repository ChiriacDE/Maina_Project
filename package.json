{
  "name": "maina-project",
  "version": "1.3.1",
  "description": "El proyecto Maina empieza a tomar forma! La version 1.3 promete disponer de `templates` para la generacion automatizada de paginas y elementos",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^v19.7.0"
  },
  "scripts": {
    "css": "node-sass --include-path scss scss/main.scss scss/main.css",
    "uglify-css": "uglifycss scss/main.css > scss/main.min.css",
    "watch": "nodemon -e scss -x 'npm run css && npm run uglify-css'",
    "build": "vite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChiriacDE/Maina_Project.git"
  },
  "author": "Andrei Chiriac",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChiriacDE/Maina_Project/issues"
  },
  "homepage": "https://github.com/ChiriacDE/Maina_Project/index.html",
  "dependencies": {
    "jquery": "^3.6.3",
    "node-sass": "^8.0.0",
    "nodemon": "^2.0.21",
    "uglifycss": "^0.0.29",
    "vite": "^4.1.4"
  }
}
